{
    "copy": {
        "page": "index-copy"
    },
    "meta": [
        {
            "name": "tracking",
            "content": "`tracking.code`"
        }
    ],
    "header": {
        "h1Text": "`page.headerH1Text`"
    },
    "title": "`page.title`",
    "links": [
        {
            "anchorText": "`page.firstLinkAnchorText`",
            "href": "#",
            "isButton": true
        }
    ],
    "messages": {
        "alertInfo": true,
        "alertHeader": "`messages.alerts.global.header`",
        "alertMessage": "`messages.alerts.global.message`"
    },
    "fields": [
        {
            "header": {
                "h1Text": "`page.field1.h1Text`"
            },
            "labelText": "`page.field1.labelText`",
            "optional": true
        },
        {
            "header": {
                "h1Text": "`page.field2.h1Text`"
            },
            "labelText": "`page.field2.labelText`",
            "messages": false
        },
        {
            "header": {
                "h1Text": "`page.field3.h1Text`"
            },
            "labelText": "`page.field3.labelText`",
            "messages": {
                "alertInfo": true,
                "alertHeader": "`messages.alerts.field.header`",
                "alertMessage": "`messages.alerts.field.message`"
            }
        }
    ],
    "tracking": "`tracking.url`"
}
